{
    "name": "tharindu/laravel-backup",
    "description": "A Laravel package to backup your application database and files to a cloud storage.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tharindu\\LaravelBackup\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tharindu\\LaravelBackup\\BackupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}